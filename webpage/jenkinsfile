pipeline {
     agent any
     
     stages {
       stage ('build Stage') {
            steps {
              
                           
                     sh '''
              cd webpage
              mvn clean install
    '''
               }
            }
             
         }
         
         post {
         always{
         junit 'a /webpage/target/surfire-reports/*.xml'
      }
      }
      }
      
      
          
